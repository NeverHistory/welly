{
  "name": "welly",
  "version": "0.0.0",
  "description": "A cli for creating a cloud function backend",
  "exports": "./dist/index.js",
  "type": "module",
  "bugs": {
    "url": "https://github.com/ilikesolutions/welly/issues"
  },
  "bin": {
    "welly": "dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=development tsc -w",
    "build:prod": "NODE_ENV=production tsc",
    "clean": "rm -rf dist/",
    "start": "npm run clean && npm run build",
    "postinstall": "husky install",
    "lint": "eslint src --fix",
    "lint:prod": "eslint src"
  },
  "keywords": [
    "cli",
    "aws",
    "gcs",
    "cloud-functions"
  ],
  "author": "BettaWae",
  "license": "MIT",
  "devDependencies": {
    "@types/archiver": "^5.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.2"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "chalk": "^5.0.0",
    "esbuild": "^0.14.3",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "log-symbols": "^5.1.0",
    "ora": "^6.0.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.13.1",
    "npm": ">=8.1.2"
  }
}
